{
  "name": "webpack-vue-boilerplate",
  "version": "1.0.0",
  "description": "A webpack 5 boilerplate with Vue.js",
  "main": "src/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config build/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.js",
    "preview": "http-server ./dist -a localhost -p 5000 -o",
    "lint": "eslint 'src/**/*.js' || true",
    "prettify": "prettier --write '**/*.js'"
  },
  "keywords": [
    "webpack",
    "webpack 5",
    "webpack boilerplate",
    "vue"
  ],
  "dependencies": {
    "core-js": "^3.26.1",
    "vue": "^3.0.0",
    "vue-router": "^3.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-arrow-functions": "^7.12.1",
    "@babel/plugin-transform-async-to-generator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.15.8",
    "@winner-fed/prettier-config-win": "^0.0.1",
    "babel-loader": "^9.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "http-server": "^14.1.1",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.4.2",
    "sass": "^1.43.5",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.4.1",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.0",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.58.2",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cklwblove/webpack-vue-boilerplate"
  },
  "license": "MIT"
}
